        "Arduino",
        "Ionic",
        "WordPress",
        "AngularJS",
        "Tizen OS",
        "PHP",
        "HTML",
        "CSS",
        "Illustrator",
        "Photoshop",
        "Polymer",
        "Angular",
        "JavaScript",
        "Jira",
        "CI/CD",
        "Patrones de Dise√±o",
        "Web Components",
        "Angular",
        "TypeScript",
        "SCSS",
        "GraphQL",
        "Unit Testing",
        "React",
        "Jira"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "printable": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "projects": {
      "displayName": "Projects",
      "type": "component",
